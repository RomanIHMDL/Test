<Window x:Class="DEMOEX.Промежуток"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DEMOEX"
        mc:Ignorable="d"
        Title="Промежуток" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="zak" HorizontalAlignment="Left" Height="238" Margin="10,90,0,0" VerticalAlignment="Top" Width="774"/>
        <TextBox x:Name="поиск" HorizontalAlignment="Left" Height="23" Margin="70,365,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="поиск_TextChanged"/>
        <Label Content="Номер вашего заказа будет указан последним в списке:" HorizontalAlignment="Left" Margin="0,10,-0.4,0" VerticalAlignment="Top" Height="75" Width="794"/>
        <Button Content="Продолжить" HorizontalAlignment="Left" Margin="668,379,0,0" VerticalAlignment="Top" Width="116" Height="28" Click="Button_Click"/>

    </Grid>
</Window>
